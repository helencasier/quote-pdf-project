                <targetReference>Set_Qualified_DT</targetReference>
            </connector>
            <label>Qualified</label>
        </rules>
        <rules>
            <name>Status_Recycle</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
